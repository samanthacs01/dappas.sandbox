{
  "name": "dappas-api",
  "sourceRoot": "apps/dappas-api",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/dappas-api && GO111MODULE=auto go build -o dist/dappas-api main.go"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker-compose -f docker-compose.yml up --build dappas-api -d"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml  up --build dappas-api"
      }
    },
    "gendocs": {
      "executor": "nx:run-commands",
      "options": {
        "command": "swag init --dir=apps/dappas-api --output=apps/dappas-api/docs"
      }
    },
    "test": {
      "executor": "@nx-go/nx-go:test",
      "options": {
        "buildTarget": "dappas-api:build"
      }
    },
    "test-cover": {
      "executor": "@nx-go/nx-go:test",
      "options": {
        "buildTarget": "dappas-api:build",
        "cover": true
      }
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint",
      "options": {
        "lintFilePatterns": ["apps/dappas-api/**/*.go"]
      }
    },
    "tidy": {
      "executor": "@nx-go/nx-go:tidy",
      "options": {
        "buildTarget": "dappas-api:download"
      }
    },
    "download": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/dappas-api && go mod download"
      }
    },    
    "gen": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/dappas-api && go run cmd/generator/main.go"
      }
    }
  },
  "tags": []
}
