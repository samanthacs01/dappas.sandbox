{
  "name": "dappas-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/dappas-web",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "production",
      "options": {
        "command": "nx run dappas-web:build:prod"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "nx run dappas-web:build:prod"
          },
          {
            "command": "nx run dappas-web:start:dev"
          }
        ],
        "parallel": false
      }
    }
  }
}
